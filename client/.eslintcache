   onChange={handleChange}\n        />\n        <Form.Input\n          label=\"Image\"\n          placeholder=\"Image\"\n          name=\"image\"\n          value={userData.image}\n          onChange={handleChange}\n        />\n        <Form.Input\n          label=\"GitHub\"\n          placeholder=\"GitHub Link\"\n          name=\"github_link\"\n          value={userData.github_link}\n          onChange={handleChange}\n        />\n        <Form.Input\n          label=\"Personal Site\"\n          placeholder=\"Personal Site URL\"\n          name=\"personal_site\"\n          value={userData.personal_site}\n          onChange={handleChange}\n        />\n        <Form.Input\n          label=\"Linkedin\"\n          placeholder=\"Linkedin\"\n          name=\"linkedin_link\"\n          value={userData.linkedin_link}\n          onChange={handleChange}\n        />\n        <Form.Input\n          label=\"Tag\"\n          placeholder=\"Tag\"\n          name=\"tag\"\n          value={userData.tag}\n          onChange={handleChange}\n        />\n      </Form>\n    </div>\n  );\n};\n\nexport default UserSettings;\n","/Users/sloanybones/DPL_WINTER_2020/portfolio/life-after-dev/client/src/hooks/useFormInput.js",[],"/Users/sloanybones/DPL_WINTER_2020/portfolio/life-after-dev/client/src/index.js",[],"/Users/sloanybones/DPL_WINTER_2020/portfolio/life-after-dev/client/src/hooks/useInfiniteScroll.js",["97","98","99"],{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","severity":1,"message":"105","line":4,"column":1,"nodeType":"106","endLine":26,"endColumn":2},{"ruleId":"100","replacedBy":"107"},{"ruleId":"102","replacedBy":"108"},{"ruleId":"109","severity":1,"message":"110","line":3,"column":16,"nodeType":"111","messageId":"112","endLine":3,"endColumn":20},{"ruleId":"109","severity":1,"message":"113","line":4,"column":8,"nodeType":"111","messageId":"112","endLine":4,"endColumn":20},{"ruleId":"100","replacedBy":"114"},{"ruleId":"102","replacedBy":"115