ui-react\";\nimport AuthConsumer, { AuthContext } from \"../providers/AuthProvider.js\";\n\nconst Navbar1 = (props) => {\n  let history = useHistory();\n  let { pathname } = useLocation();\n  const { user, handleLogout } = useContext(AuthContext);\n\n  const rightNavItems = () => {\n    if (user) {\n      return (\n        <Menu.Menu position=\"right\">\n          <Menu.Item\n            onClick={() => history.push(`/profile/${user.id}/settings`)}\n            icon=\"setting\"\n          />\n\n          <Menu.Item name=\"Logout\" onClick={() => handleLogout(history)} />\n        </Menu.Menu>\n      );\n    } else {\n      return (\n        <Menu.Menu position=\"right\">\n          <Link to=\"/login\">\n            <Menu.Item id=\"login\" name=\"login\" active={pathname === \"/login\"} />\n          </Link>\n          <Link to=\"/register\">\n            <Menu.Item\n              id=\"register\"\n              name=\"register\"\n              active={pathname === \"/register\"}\n            />\n          </Link>\n        </Menu.Menu>\n      );\n    }\n  };\n  return (\n    <div>\n      <Menu pointing secondary>\n        <Link to=\"/\">\n          <Menu.Item name=\"home\" id=\"home\" active={pathname === \"/\"} />\n        </Link>\n        {rightNavItems()}\n      </Menu>\n    </div>\n  );\n};\n\nexport default Navbar1;\n",["82","83"],"/Users/sloanybones/DPL_WINTER_2020/portfolio/life-after-dev/client/src/Hooks/useFormInput.js",[],"/Users/sloanybones/DPL_WINTER_2020/portfolio/life-after-dev/client/src/App.js",["84","85","86","87"],"/Users/sloanybones/DPL_WINTER_2020/portfolio/life-after-dev/client/src/demo/Home.js",["88"],"import {Header} from \"semantic-ui-react\"\nexport default () => <Header>Home</Header>","/Users/sloanybones/DPL_WINTER_2020/portfolio/life-after-dev/client/src/components/Register.js",["89","90"],"import React, { useState, useContext, useEffect } from \"react\";\nimport { AuthContext } from \"../providers/AuthProvider\";\nimport { Button, Form, Segment, Header } from \"semantic-ui-react\";\nimport { useFormInput } from \"../hooks/useFormInput\";\n\nconst Register = ({ history }) => {\n  // const [email, setEmail] = useState(\"\");\n  // const [password, setPassword] = useState(\"\");\n  // const [passwordConfirmation, setPasswordConfirmation] = useState(\"\");\n  const { handleRegister } = useContext(AuthContext);\n  const email = useFormInput(\"\", \"Email\");\n  const password = useFormInput(\"\", \"Password\");\n  const passwordConfirmation = useFormInput(\"\", \"Password Confirmation\");\n\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    if (password.value === passwordConfirmation.value)\n      handleRegister(\n        {\n          email: email.value,\n          password: password.value,\n          passwordConfirmation: passwordConfirmation.value,\n        },\n        history\n      );\n    else alert(\"Passwords Do Not Match!\");\n  };\n\n\n  return (\n    <Segment basic>\n      <Header as=\"h1\" textAlign=\"center\">\n        Register\n      </Header>\n      <Form onSubmit={handleSubmit}>\n        <Form.Input autoFocus {...email} />\n        <Form.Input type=\"password\" {...password} />\n        <Form.Input type=\"password\" {...passwordConfirmation} />\n        <Segment textAlign=\"center\" basic>\n          <Button\n            primary\n            type=\"submit\"\n          >\n            Submit\n          </Button>\n        </Segment>\n      </Form>\n    </Segment>\n  );\n};\n\nexport default Register;","/Users/sloanybones/DPL_WINTER_2020/portfolio/life-after-dev/client/src/providers/AuthProvider.js",[],"/Users/sloanybones/DPL_WINTER_2020/portfolio/life-after-dev/client/src/components/Login.js",["91"],"/Users/sloanybones/DPL_WINTER_2020/portfolio/life-after-dev/client/src/components/ProtectedRoute.js",["92"],"\nimport React, { Component, useContext } from \"react\";\nimport { Route, Redirect } from \"react-router-dom\";\nimport { AuthContext } from \"../providers/AuthProvider\";\n\n\