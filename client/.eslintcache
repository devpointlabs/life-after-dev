ve(false);\n    setProfileActive(false);\n    setSettingsActive(true);\n  };\n\n  const renderHomeIcon = () => {\n    if (!homeActive) {\n      return (\n        <NavInactiveSquare onClick={() => homeToggle()}>\n          <NavInactiveIcon src={homeicon} />\n        </NavInactiveSquare>\n      );\n    } else if (homeActive) {\n      return (\n        <>\n          <NavActiveSquare>\n            <NavActiveIcon src={homeicon} />\n          </NavActiveSquare>\n        </>\n      );\n    }\n  };\n\n  const renderProfileIcon = () => {\n    if (!profileActive) {\n      return (\n        <NavInactiveSquare onClick={() => profileToggle()}>\n          <NavInactiveIcon src={profileicon} />\n        </NavInactiveSquare>\n      );\n    } else if (profileActive) {\n      return (\n        <NavActiveSquare>\n          <NavActiveIcon src={profileicon} />\n        </NavActiveSquare>\n      );\n    }\n  };\n\n  const renderSettingsIcon = () => {\n    if (!settingsActive) {\n      return (\n        <NavInactiveSquare onClick={() => settingsToggle()}>\n          <NavInactiveIcon src={settingsicon} />\n        </NavInactiveSquare>\n      );\n    } else if (settingsActive) {\n      return (\n        <NavActiveSquare>\n          <NavActiveIcon src={settingsicon} />\n        </NavActiveSquare>\n      );\n    }\n  };\n\n  const renderAddIcon = () => {};\n\n  const topNavItems = () => {\n    if (user) {\n      return (\n        <>\n          <Link to={`/user/${user.id}`}>{renderProfileIcon()}</Link>\n          <NavInactiveSquare>\n            <NavInactiveIcon src={addicon} />\n          </NavInactiveSquare>\n          <Link to={`/profile/${user.id}/settings`}>\n            {renderSettingsIcon()}\n          </Link>\n\n          <Link>\n            {\n              <NavIconBottomSquare>\n                <NavIconBottomInactive\n                  src={logouticon}\n                  onClick={() => handleLogout(history)}\n                />\n              </NavIconBottomSquare>\n            }\n          </Link>\n        </>\n      );\n    } else {\n      return (\n        <>\n          <Link to=\"/login\">\n            <AuthButton id=\"login\" name=\"login\" active={pathname === \"/login\"}>\n              Login\n            </AuthButton>\n          </Link>\n          <Link to=\"/register\">\n            <AuthButton\n              id=\"register\"\n              name=\"register\"\n              active={pathname === \"/register\"}\n            >\n              Register\n            </AuthButton>\n          </Link>\n        </>\n      );\n    }\n  };\n\n  return (\n    <NavColumn>\n      <StickyNav>\n        <Link to=\"/\">\n          <Logo src={devpointlogo} />\n        </Link>\n        <Link to=\"/\">{renderHomeIcon()}</Link>\n        {topNavItems()}\n      </StickyNav>\n    </NavColumn>\n  );\n};\n\nexport default Navbar;\n",["184","185"],"/Users/ian/Desktop/final-proj/client/src/components/Dropzone.js",["186","187"],"import Axios from \"axios\";\nimport { useState, useCallback, useContext } from \"react\";\nimport { useDropzone } from \"react-dropzone\";\nimport useCustomDrop from \"../hooks/useCustomDrop\";\nimport { AuthContext } from \"../providers/AuthProvider\";\n\nexport default (props) => {\n  const [image, setImage] = useState(null);\n  const {updateProfile} = useCustomDrop()\n  const { user } = useContext(AuthContext);\n\n  const onDrop = useCallback((acceptedFiles) => {\n    // Do something with the files\n    setImage(acceptedFiles[0]);\n  }, []);\n\n  const { getRootProps, getInputProps, isDragActive } = useDropzone({ onDrop });\n  return ( \n    <>\n      <form onSubmit={() => updateProfile(user, image)}>\n        \n        <p>image</p>\n        <div {...getRootProps()}>\n          <input {...getInputProps()} />\n          {isDragActive ? (\n            <p>Drop the files here ...</p>\n          ) : (\n            <p>Drag 'n' drop some files here, or click to select files</p>\n          )}\n        </div>\n        <button type=\"submit\">add</bu